{
  "name": "gpu-performance-monitor-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^10",
    "chart.js": "^4",
    "concurrently": "latest",
    "postcss": "^8",
    "react": "^18",
    "react-chartjs-2": "^5",
    "react-dom": "^18",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3"
  },
  "scripts": {
    "start": "concurrently \"npm run build:css:watch\" \"react-scripts start\"",
